### Session / User specific settings
#

### Auto-Start
#

# Policy Kit
exec /usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1

# Notifications
exec mkdir -p "$XDG_RUNTIME_DIR/notifications"
exec mako

# Launcher Daemon
exec $HOME/Scripts/qutepy_launcher.py -k && $HOME/Scripts/list_desktop_applications.sh | $HOME/Scripts/qutepy_launcher.py -d -f -l -n

# File Manager Daemon
#exec thunar --daemon
exec pcmanfm-qt -d

